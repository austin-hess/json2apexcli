{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "governingLawState": { "type": "string" },
    "transactionDate": { "type": "string" },
    "applicationDate": { "type": "string" },
    "activityType": { "type": "integer" },
    "isCommunityProperty": { "type": "boolean" },
    "applicants": {
      "type": "object",
      "properties": {
        "individuals": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "ID": { "type": "string" },
              "firstName": { "type": "string" },
              "lastName": { "type": "string" },
              "middleName": { "type": "string" },
              "suffixName": { "type": "string" },
              "address": {
                "type": "object",
                "properties": {
                  "primaryAddressLine": { "type": "string" },
                  "city": { "type": "string" },
                  "state": { "type": "string" },
                  "postalCode": { "type": "string" }
                },
                "required": [
                  "city",
                  "postalCode",
                  "primaryAddressLine",
                  "state"
                ]
              },
              "ssn": { "type": "string" },
              "dateOfBirth": { "type": "string" },
              "tinStatus": { "type": "integer" },
              "previousAddresses": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "primaryAddressLine": { "type": "string" },
                    "city": { "type": "string" },
                    "state": { "type": "string" },
                    "postalCode": { "type": "string" }
                  },
                  "required": [
                    "city",
                    "postalCode",
                    "primaryAddressLine",
                    "state"
                  ]
                }
              },
              "employers": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": { "type": "string" },
                    "address": {
                      "type": "object",
                      "properties": {
                        "primaryAddressLine": { "type": "string" },
                        "city": { "type": "string" },
                        "state": { "type": "string" },
                        "postalCode": { "type": "string" }
                      },
                      "required": [
                        "city",
                        "postalCode",
                        "primaryAddressLine",
                        "state"
                      ]
                    },
                    "isSelfEmployment": { "type": "integer" },
                    "lengthOfEmploymentMonths": { "type": "string" }
                  },
                  "required": [
                    "address",
                    "isSelfEmployment",
                    "lengthOfEmploymentMonths",
                    "name"
                  ]
                }
              },
              "previousEmployers": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": { "type": "string" },
                    "address": {
                      "type": "object",
                      "properties": {
                        "primaryAddressLine": { "type": "string" },
                        "city": { "type": "string" },
                        "state": { "type": "string" },
                        "postalCode": { "type": "string" }
                      },
                      "required": [
                        "city",
                        "postalCode",
                        "primaryAddressLine",
                        "state"
                      ]
                    },
                    "isSelfEmployment": { "type": "integer" }
                  },
                  "required": ["address", "isSelfEmployment", "name"]
                }
              },
              "identifications": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "description": { "type": "string" },
                    "number": { "type": "string" },
                    "issuer": { "type": "string" },
                    "issueDate": { "type": "string" },
                    "expirationDate": { "type": "string" }
                  },
                  "required": [
                    "description",
                    "expirationDate",
                    "issueDate",
                    "issuer",
                    "number"
                  ]
                }
              },
              "phones": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "integer" },
                    "number": { "type": "string" }
                  },
                  "required": ["number", "type"]
                }
              },
              "isOFACVerified": { "type": "boolean" },
              "spouseID": { "type": "string" },
              "isPrimaryOwner": { "type": "boolean" },
              "isCIVDocumentRequired": { "type": "boolean" },
              "ATMDebitCard": {
                "type": "object",
                "properties": {
                  "type": { "type": "integer" },
                  "number": { "type": "string" },
                  "cardholderStatus": { "type": "integer" },
                  "cardStatus": { "type": "integer" },
                  "linkedAccounts": {
                    "type": "array",
                    "items": { "type": "string" }
                  }
                },
                "required": [
                  "cardStatus",
                  "cardholderStatus",
                  "linkedAccounts",
                  "number",
                  "type"
                ]
              },
              "attorneysInFact": {
                "type": "array",
                "items": { "type": "string" }
              },
              "isMinor": { "type": "boolean" }
            },
            "required": [
              "ID",
              "address",
              "firstName",
              "lastName",
              "ssn",
              "tinStatus"
            ]
          }
        },
        "businesses": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "ID": { "type": "string" },
              "name": { "type": "string" },
              "entityType": { "type": "integer" },
              "tin": { "type": "string" },
              "parentCompanyIDs": { "type": "string" },
              "dba": { "type": "string" },
              "natureOfBusiness": { "type": "string" },
              "establishedDate": { "type": "string" },
              "isNonProfit": { "type": "boolean" },
              "resolutionOnFileType": { "type": "integer" },
              "principalOfficeState": { "type": "string" },
              "principalOfficeCity": { "type": "string" },
              "address": {
                "type": "object",
                "properties": {
                  "primaryAddressLine": { "type": "string" },
                  "state": { "type": "string" },
                  "city": { "type": "string" },
                  "postalCode": { "type": "string" }
                },
                "required": [
                  "city",
                  "postalCode",
                  "primaryAddressLine",
                  "state"
                ]
              },
              "isEngagesInInternetGambling": { "type": "boolean" },
              "signers": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "ID": { "type": "string" },
                    "firstName": { "type": "string" },
                    "middleName": { "type": "string" },
                    "lastName": { "type": "string" },
                    "suffixName": { "type": "string" },
                    "address": {
                      "type": "object",
                      "properties": {
                        "primaryAddressLine": { "type": "string" },
                        "state": { "type": "string" },
                        "city": { "type": "string" },
                        "postalCode": { "type": "string" }
                      },
                      "required": [
                        "city",
                        "postalCode",
                        "primaryAddressLine",
                        "state"
                      ]
                    },
                    "dateOfBirth": { "type": "string" },
                    "ssn": { "type": "string" },
                    "isDocumentSigner": { "type": "boolean" },
                    "isResolutionSigner": { "type": "boolean" },
                    "isResolutionCertifier": { "type": "boolean" },
                    "isBeneficialOwner": { "type": "boolean" },
                    "isBeneficialOwnerManager": { "type": "boolean" },
                    "isBeneficialOwnerCertifier": { "type": "boolean" },
                    "isTINCertifier": { "type": "boolean" },
                    "capacityDescription": { "type": "string" },
                    "isTrustee": { "type": "boolean" },
                    "isTrustBeneficiary": { "type": "boolean" },
                    "isTrustCreator": { "type": "boolean" }
                  },
                  "required": [
                    "ID",
                    "address",
                    "capacityDescription",
                    "dateOfBirth",
                    "firstName",
                    "isBeneficialOwner",
                    "isBeneficialOwnerCertifier",
                    "isBeneficialOwnerManager",
                    "isTINCertifier",
                    "lastName",
                    "ssn"
                  ]
                }
              }
            },
            "required": [
              "ID",
              "address",
              "dba",
              "entityType",
              "establishedDate",
              "isEngagesInInternetGambling",
              "name",
              "natureOfBusiness",
              "parentCompanyIDs",
              "principalOfficeCity",
              "principalOfficeState",
              "signers",
              "tin"
            ]
          }
        }
      }
    },
    "accounts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "ID": { "type": "integer" },
          "number": { "type": "string" },
          "type": { "type": "integer" },
          "ownershipStructure": { "type": "integer" },
          "ownershipType": { "type": "integer" },
          "primaryOwnerID": { "type": "string" },
          "productCode": { "type": "string" },
          "productDescription": { "type": "string" },
          "owners": { "type": "array", "items": { "type": "string" } },
          "rateStructure": { "type": "integer" },
          "rateType": { "type": "integer" },
          "compoundingFrequency": { "type": "integer" },
          "creditingFrequency": { "type": "integer" },
          "interestPaymentMethod": { "type": "integer" },
          "rates": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "interestRate": { "type": "number" },
                "apy": { "type": "number" }
              },
              "required": ["apy", "interestRate"]
            }
          },
          "depositAmount": { "type": "number" },
          "fiduciaries": { "type": "array", "items": { "type": "string" } },
          "statementMailingAddress": {
            "type": "object",
            "properties": {
              "primaryAddressLine": { "type": "string" },
              "city": { "type": "string" },
              "state": { "type": "string" },
              "postalCode": { "type": "string" }
            },
            "required": ["city", "postalCode", "primaryAddressLine", "state"]
          },
          "authorizedSigners": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "ID": { "type": "string" },
                "isDurable": { "type": "boolean" }
              },
              "required": ["ID", "isDurable"]
            }
          },
          "beneficiaries": { "type": "array", "items": { "type": "string" } },
          "safeDepositBox": {
            "type": "object",
            "properties": {
              "size": { "type": "string" },
              "numberOfKeys": { "type": "integer" },
              "numberOfRequiredRentersToAccess": { "type": "integer" },
              "deputies": { "type": "array", "items": { "type": "string" } },
              "appointees": { "type": "array", "items": { "type": "string" } }
            },
            "required": [
              "appointees",
              "deputies",
              "numberOfKeys",
              "numberOfRequiredRentersToAccess",
              "size"
            ]
          },
          "custodians": { "type": "array", "items": { "type": "string" } },
          "successorCustodians": {
            "type": "array",
            "items": { "type": "string" }
          }
        },
        "required": [
          "ID",
          "number",
          "owners",
          "ownershipStructure",
          "primaryOwnerID",
          "productCode",
          "productDescription",
          "type"
        ]
      }
    },
    "financialInstitution": {
      "type": "object",
      "properties": {
        "name": { "type": "string" },
        "address": {
          "type": "object",
          "properties": {
            "primaryAddressLine": { "type": "string" },
            "city": { "type": "string" },
            "state": { "type": "string" },
            "postalCode": { "type": "string" }
          },
          "required": ["city", "postalCode", "primaryAddressLine", "state"]
        },
        "decisionOfficer": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "title": { "type": "string" }
          },
          "required": ["name", "title"]
        },
        "documentSigner": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "title": { "type": "string" }
          },
          "required": ["name", "title"]
        },
        "documentPreparer": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "title": { "type": "string" }
          },
          "required": ["name", "title"]
        },
        "openingBranch": {
          "type": "object",
          "properties": {
            "name": { "type": "string" },
            "code": { "type": "string" },
            "address": {
              "type": "object",
              "properties": {
                "primaryAddressLine": { "type": "string" },
                "city": { "type": "string" },
                "state": { "type": "string" },
                "postalCode": { "type": "string" }
              },
              "required": ["city", "postalCode", "primaryAddressLine", "state"]
            }
          },
          "required": ["address", "code", "name"]
        }
      },
      "required": [
        "address",
        "decisionOfficer",
        "documentPreparer",
        "documentSigner",
        "name",
        "openingBranch"
      ]
    },
    "contacts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "ID": { "type": "string" },
          "entityType": { "type": "string" },
          "firstName": { "type": "string" },
          "lastName": { "type": "string" },
          "businessName": { "type": "string" },
          "title": { "type": "string" },
          "address": {
            "type": "object",
            "properties": {
              "primaryAddressLine": { "type": "string" },
              "city": { "type": "string" },
              "state": { "type": "string" },
              "postalCode": { "type": "string" }
            },
            "required": ["city", "postalCode", "primaryAddressLine", "state"]
          }
        },
        "required": [
          "ID",
          "address",
          "businessName",
          "entityType",
          "firstName",
          "lastName",
          "title"
        ]
      }
    }
  },
  "required": [
    "accounts",
    "activityType",
    "applicants",
    "applicationDate",
    "financialInstitution",
    "governingLawState",
    "transactionDate"
  ]
}
